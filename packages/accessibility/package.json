{
  "name": "@tbcm/accessibility",
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "debug": "DEBUG=true pa11y-ci --config ./src/pa11y-ci.js",
    "start": "echo 'Run accessibility tests...' && pa11y-ci --config ./src/pa11y-ci.js --json > out/accessibility_results.json",
